<?php
$vote = $_GET['result'];
echo "woot it works ajax style";

$post = Post::get();
if ($vote == 1) {
$post->info->r1 ++;
}
if ($vote == 2) {
$post->info->r2 ++;
}
if ($vote == 3) {
$post->info->r3 ++;
}
if ($vote == 4) {
$post->info->r4 ++;
}
if ($vote == 5) {
$post->info->r5s ++;
}
if ($vote == 'null') {
//no vote, just veiw.
}
?>
